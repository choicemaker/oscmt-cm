<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.choicemaker.mvn.cluemaker.unit</groupId>
	<artifactId>project-to-test</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>jar</packaging>
	<name>Test #1 ClueMaker Maven Plugin</name>

	<properties>
		<aetherVersion>1.0.0.v20140518</aetherVersion>
	</properties>

	<build>
		<plugins>
			<plugin>
				<groupId>com.choicemaker.mvn.cluemaker</groupId>
				<artifactId>cluemaker-maven-plugin</artifactId>
				<version>1.0-SNAPSHOT</version>
				<configuration>
					<cluemakerDirectory>src/test/resources/cluemaker</cluemakerDirectory>
        <!--
					<generatedSourceDirectory>${project.build.directory}/generated-sources</generatedSourceDirectory>
					<compiledCodeDirectory>${project.build.outputDirectory}</compiledCodeDirectory>
        -->
					<generators>
						<generator>
							com.choicemaker.cm.compiler.gen.GuiDescriptorGenerator
						</generator>
						<generator>
							com.choicemaker.cm.io.blocking.automated.gen.BlockingGenerator
						</generator>
						<generator>
							com.choicemaker.cm.io.blocking.exact.base.gen.BlockingGenerator
						</generator>
						<generator>
							com.choicemaker.cm.io.db.base.gen.DbGenerator
						</generator>
						<generator>
							com.choicemaker.cm.io.flatfile.base.gen.FlatFileGenerator
						</generator>
						<generator>
							com.choicemaker.cm.io.xml.base.gen.XmlGenerator
						</generator>
					</generators>
				</configuration>
				<executions>
					<execution>
						<id>generate-test-1</id>
						<phase>process-sources</phase>
						<goals>
							<goal>generate</goal>
						</goals>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.compiler</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.core</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.io.blocking.automated.base</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.io.blocking.exact.base</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.io.db.base</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.io.flatfile.base</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.io.xml.base</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.matching.cfg</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.matching.en</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.matching.en.au</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.choicemaker.cm</groupId>
						<artifactId>com.choicemaker.cm.matching.en.us</artifactId>
						<version>2.7.1-SNAPSHOT</version>
					</dependency>
				</dependencies>
			</plugin>
		</plugins>
	</build>
</project>
