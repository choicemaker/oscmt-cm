<?xml version="1.0" encoding="UTF-8"?>
<fragment
   id="com.choicemaker.cm.modelmaker.std"
   name="Modelmaker Plug-in, Standard E2 Fragment"
   version="2.5.0"
   provider-name="ChoiceMaker LLC"
   plugin-id="com.choicemaker.cm.modelmaker"
   plugin-version="2.5.0">

  <runtime>
    <library name="mmstd.jar">
      <export name="*"/>
    </library>
  </runtime>

   <requires>
      <import plugin="org.eclipse.core.boot"/>
      <import plugin="org.eclipse.core.runtime"/>
   </requires>

   <extension
         id="ModelMakerStd"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.choicemaker.cm.modelmaker.gui.ModelMakerStd">
         </run>
      </application>
   </extension>

</fragment>
