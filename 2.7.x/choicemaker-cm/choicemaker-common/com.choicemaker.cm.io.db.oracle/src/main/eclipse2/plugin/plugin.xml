<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin>
<plugin
   id="com.choicemaker.cm.io.db.oracle"
   name="Oracle DB IO Plug-in"
   version="2.5.0"
   provider-name="ChoiceMaker">

   <runtime>
      <library name="oracle.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="com.choicemaker.cm.core"/>
      <import plugin="com.choicemaker.cm.io.db.base"/>
      <import plugin="com.choicemaker.cm.aba.base"/>
   </requires>


   <extension
         id="OraDbObjectMaker"
         point="com.choicemaker.e2.applications">
      <application>
         <run
               class="com.choicemaker.cm.io.db.oracle.dbom.OracleDbObjectMaker">
         </run>
      </application>
   </extension>
   <extension
         point="com.choicemaker.cm.core.objectGenerator">
      <generator
            description="Oracle Custom Objects (DDL)"
            class="com.choicemaker.cm.io.db.oracle.dbom.OracleDbObjectMaker">
      </generator>
   </extension>
   <extension
         id="oracleDatabaseAccessor"
         name="oracleDatabaseAccessor"
         point="com.choicemaker.cm.aba.base.databaseAccessor">
      <databaseAccessor
            class="com.choicemaker.cm.io.db.oracle.blocking.OraDatabaseAccessor">
      </databaseAccessor>
   </extension>
   <extension
         id="oracleDatabaseAbstraction"
         name="oracleDatabaseAbstraction"
         point="com.choicemaker.cm.io.db.base.databaseAbstraction">
      <databaseAbstraction
            class="com.choicemaker.cm.io.db.oracle.blocking.OraDatabaseAbstraction">
      </databaseAbstraction>
   </extension>
   <extension
         id="oracleRecordSourceSerializer"
         name="Oracle Record-Source Serializer"
         point="com.choicemaker.cm.core.rsSerializer">
      <serializer
            class="com.choicemaker.cm.io.db.oracle.OracleRecordSourceSerializer">
      </serializer>
   </extension>

</plugin>
