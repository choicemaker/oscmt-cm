<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="person-matching-models"
   name="Person-Matching Plugin Models"
   version="2.7.1"
   provider-name="ChoiceMaker">

   <runtime>
      <library name="models.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="com.choicemaker.cm.core"/>
   </requires>

   <extension
         point="com.choicemaker.cm.core.modelConfiguration">
      <modelConfiguration
            name="modelConfig1"
            file="/SimplePerson.model"
            dbConfig="default"
            blockingConfig="default"/>
      <modelConfiguration
            name="modelConfig2"
            file="/SimplePerson2.model"
            dbConfig="default"
            blockingConfig="default"/>
   </extension>

</plugin>
