      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version><!--$NO-MVN-MAN-VER$-->
        <configuration>
          <verbose>true</verbose>
					<!-- fork>true</fork -->
          <fork>false</fork>
          <compilerVersion>1.7</compilerVersion>
          <source>1.7</source>
          <target>1.7</target>
          <compilerArgument>-g</compilerArgument>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.choicemaker.fake</groupId>
        <artifactId>mymojo-maven-plugin</artifactId>
        <version>1.0-SNAPSHOT</version>
        <configuration />
        <executions>
          <execution>
            <id>generate</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>generate</goal>
            </goals>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.blocking.automated.base</artifactId>
            <version>2.7.1-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.blocking.exact.base</artifactId>
            <version>2.7.1-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.db.base</artifactId>
            <version>2.7.1-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.flatfile.base</artifactId>
            <version>2.7.1-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.xml.base</artifactId>
            <version>2.7.1-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.matching.en</artifactId>
            <version>2.7.1-SNAPSHOT</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.matching.en.us</artifactId>
            <version>2.7.1-SNAPSHOT</version>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
    <pluginManagement>
      <plugins>
        <!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
        <plugin>
          <groupId>org.eclipse.m2e</groupId>
          <artifactId>lifecycle-mapping</artifactId>
          <version>1.0.0</version>
          <configuration>
            <lifecycleMappingMetadata>
              <pluginExecutions>
                <pluginExecution>
                  <pluginExecutionFilter>
                    <groupId>com.choicemaker.fake</groupId>
                    <artifactId>mymojo-maven-plugin</artifactId>
                    <versionRange>[1.0-SNAPSHOT,)</versionRange>
                    <goals>
                      <goal>generate</goal>
                    </goals>
                  </pluginExecutionFilter>
                  <action>
                    <ignore></ignore>
                  </action>
                </pluginExecution>
              </pluginExecutions>
            </lifecycleMappingMetadata>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
