  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>com.choicemaker.cm</groupId>
    <artifactId>choicemaker-base</artifactId>
    <version>2.7.1-SNAPSHOT</version>
    <relativePath>../choicemaker-base/pom.xml</relativePath>
  </parent>

  <artifactId>choicemaker-cm</artifactId>
  <packaging>pom</packaging>

  <name>ChoiceMaker Record Matching</name>
  <url>http://oscmt.sourceforge.net/dev-doc</url>

  <scm>
    <!-- connection>scm:git:ssh://git.code.sf.net/p/oscmt/cm.git</connection -->
    <url>http://sourceforge.net/p/oscmt/cm/ci/master/tree/2.7.x/</url>
    <!-- developerConnection>scm:git:ssh://git.code.sf.net/p/oscmt/cm.git</developerConnection -->
  </scm>

  <issueManagement>
    <url>http://sourceforge.net/projects/oscmt/support</url>
  </issueManagement>

  <build>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
      </resource>
      <resource>
        <directory>src/main/eclipse2</directory>
        <targetPath>META-INF/plugins</targetPath>
      </resource>
    </resources>

    <plugins>

      <plugin>
        <artifactId>maven-site-plugin</artifactId>
        <configuration>
          <skip>${maven.javadoc.skip}</skip>
          <skipDeploy>${maven.javadoc.skip}</skipDeploy>
        </configuration>
      </plugin>

    </plugins>
  </build>

  <modules>
    <module>choicemaker-prerequisites</module>
    <module>choicemaker-common</module>
    <module>choicemaker-encryption</module>
    <module>choicemaker-modeling</module>
    <module>choicemaker-swing</module>
    <module>choicemaker-j2ee</module>
    <module>model_plugins</module>
  </modules>

