<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
>
  <modelVersion>4.0.0</modelVersion>

  <groupId>com.choicemaker.mvn.cluemaker</groupId>
  <artifactId>smoke-test</artifactId>
  <version>1.0-rc.3</version>
  <packaging>jar</packaging>
  <name>ClueMaker Maven Plugin Smoke Test</name>

  <dependencies>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.core</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.io.blocking.automated.base</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.io.blocking.exact.base</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.io.db.base</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.io.flatfile.base</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.io.xml.base</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.matching.cfg</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.matching.en</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.matching.en.au</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.matching.en.us</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.validation.eclipse</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>com.choicemaker.cm</groupId>
      <artifactId>com.choicemaker.cm.urm.base</artifactId>
      <version>2.7.1-rc.3</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <verbose>true</verbose>
					<!-- fork>true</fork -->
          <fork>false</fork>
          <compilerVersion>1.7</compilerVersion>
          <source>1.7</source>
          <target>1.7</target>
          <compilerArgument>-g</compilerArgument>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.choicemaker.mvn.cluemaker</groupId>
        <artifactId>cluemaker-maven-plugin</artifactId>
        <version>1.0-rc.3</version>
        <configuration />
        <executions>
          <execution>
            <id>generate</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>generate</goal>
            </goals>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.blocking.automated.base</artifactId>
            <version>2.7.1-rc.3</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.blocking.exact.base</artifactId>
            <version>2.7.1-rc.3</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.db.base</artifactId>
            <version>2.7.1-rc.3</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.flatfile.base</artifactId>
            <version>2.7.1-rc.3</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.io.xml.base</artifactId>
            <version>2.7.1-rc.3</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.matching.en</artifactId>
            <version>2.7.1-rc.3</version>
          </dependency>
          <dependency>
            <groupId>com.choicemaker.cm</groupId>
            <artifactId>com.choicemaker.cm.matching.en.us</artifactId>
            <version>2.7.1-rc.3</version>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
  </build>
</project>
