<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="com.choicemaker.cm.matching.intl"
   name="Intl Plug-in"
   version="2.5.0"
   provider-name="ChoiceMaker">

   <runtime>
      <library name="intl.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="com.choicemaker.cm.core"/>
      <import plugin="com.choicemaker.cm.matching.gen"/>
      <import plugin="com.choicemaker.cm.matching.cfg"/>
      <import plugin="com.choicemaker.cm.matching.en.us"/>
   </requires>


   <extension
         point="com.choicemaker.cm.matching.gen.map">
      <fileMap
            singleLine="true"
            name="intl.countryNamesToInternetCodes"
            file="etc/data/intl.countryNamesToInternetCodes.txt">
      </fileMap>
   </extension>
   <extension
         point="com.choicemaker.cm.matching.gen.set">
      <fileSet
            singleLine="true"
            name="au.country"
            file="etc/data/au.country.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="au.states"
            file="etc/data/au.states.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="br.country"
            file="etc/data/br.country.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="br.states"
            file="etc/data/br.states.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="ca.country"
            file="etc/data/ca.country.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="ca.provinces"
            file="etc/data/ca.provinces.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="ec.country"
            file="etc/data/ec.country.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="ec.provinces"
            file="etc/data/ec.provinces.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="mx.country"
            file="etc/data/mx.country.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="mx.states"
            file="etc/data/mx.states.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="uk.country"
            file="etc/data/uk.country.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="uk.regions"
            file="etc/data/uk.regions.txt">
      </fileSet>
      <fileSet
            singleLine="true"
            name="us.states"
            file="etc/data/us.states.txt">
      </fileSet>
   </extension>
   <extension
         point="com.choicemaker.cm.matching.gen.map">
      <fileMap
            singleLine="true"
            name="au.standardCountry"
            file="etc/data/au.country.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="au.standardStates"
            file="etc/data/au.states.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="br.standardCountry"
            file="etc/data/br.country.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="br.standardStates"
            file="etc/data/br.states.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="ca.standardCountry"
            file="etc/data/ca.country.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="ca.standardProvinces"
            file="etc/data/ca.provinces.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="ec.standardCountry"
            file="etc/data/ec.country.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="ec.standardProvinces"
            file="etc/data/ec.provinces.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="mx.standardCountry"
            file="etc/data/mx.country.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="mx.standardStates"
            file="etc/data/mx.states.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="uk.standardCountry"
            file="etc/data/uk.country.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="uk.standardRegions"
            file="etc/data/uk.regions.txt">
      </fileMap>
      <fileMap
            singleLine="true"
            name="us.standardStates"
            file="etc/data/us.states.txt">
      </fileMap>
   </extension>
   <extension
         point="com.choicemaker.cm.matching.cfg.parser">
      <parser
            name="au.lastLineParser"
            file="etc/parsers/au.lastLineParser.xml">
      </parser>
      <parser
            name="br.lastLineParser"
            file="etc/parsers/br.lastLineParser.xml">
      </parser>
      <parser
            name="ca.lastLineParser"
            file="etc/parsers/ca.lastLineParser.xml">
      </parser>
      <parser
            name="ec.lastLineParser"
            file="etc/parsers/ec.lastLineParser.xml">
      </parser>
      <parser
            name="mx.lastLineParser"
            file="etc/parsers/mx.lastLineParser.xml">
      </parser>
      <parser
            name="uk.lastLineParser"
            file="etc/parsers/uk.lastLineParser.xml">
      </parser>
      <parser
            name="us.lastLineParser"
            file="etc/parsers/us.lastLineParser.xml">
      </parser>
   </extension>

</plugin>
